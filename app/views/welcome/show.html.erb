<h1>All Students</h2>
<p>
 The time is now: <%= Time.now %>
</p>

<table>
  <tr>
    <th>UIN</th>
    <th>Student Name</th>
    <th>Section</th>
    <th>Attempts</th>
    <th>Score</th>

   <% @students.each do |student| %>
  <tr>
    <td><%= student.uin %></td>
    <td><%= student.name %></td>
    <td><%= student.section %></td>
    <td><%= student.attempts %></td>
    <td><%= student.score %></td>
  </tr>
  <% end %>

</table>